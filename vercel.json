{
  "installCommand": "npm install",

  "functions": {
    "api/**/*.js": {
      "runtime": "vercel-nodejs@4.0.0",
      "maxDuration": 10
    },
    "proxy.js": {
      "runtime": "vercel-nodejs@4.0.0",
      "maxDuration": 10
    }
  },

  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}
